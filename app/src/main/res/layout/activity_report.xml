<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.maxxrodriguez.test.report">

    <TextView android:text="Be sure to measure pain level of each symptom. If symptom not present then put a 0. If symptom not listed, write in other." android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#ff000000"
        android:id="@+id/textView2" />

    <SeekBar
        android:layout_width="@dimen/dialog_fixed_width_major"
        android:layout_height="wrap_content"
        android:id="@+id/seekBar"
        android:max="10"
        android:layout_above="@+id/textView4"
        android:layout_alignLeft="@+id/textView4"
        android:layout_alignStart="@+id/textView4" />

    <SeekBar
        android:layout_width="@dimen/dialog_fixed_width_major"
        android:layout_height="wrap_content"
        android:id="@+id/seekBar2"
        android:max="10"
        android:layout_above="@+id/textView5"
        android:layout_centerHorizontal="true" />

    <SeekBar
        android:layout_width="@dimen/dialog_fixed_width_major"
        android:layout_height="wrap_content"
        android:id="@+id/seekBar3"
        android:max="10"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true" />

    <SeekBar
        android:layout_width="@dimen/dialog_fixed_width_major"
        android:layout_height="wrap_content"
        android:id="@+id/seekBar4"
        android:max="10"
        android:layout_below="@+id/textView6"
        android:layout_alignLeft="@+id/seekBar3"
        android:layout_alignStart="@+id/seekBar3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Nausea"
        android:id="@+id/textView3"
        android:textColor="#ff000000"
        android:layout_above="@+id/seekBar"
        android:layout_alignLeft="@+id/seekBar"
        android:layout_alignStart="@+id/seekBar" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Headache"
        android:id="@+id/textView4"
        android:textColor="#ff000000"
        android:layout_above="@+id/seekBar2"
        android:layout_alignLeft="@+id/seekBar2"
        android:layout_alignStart="@+id/seekBar2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Chest Pain"
        android:id="@+id/textView5"
        android:textColor="#ff000000"
        android:layout_above="@+id/seekBar3"
        android:layout_alignLeft="@+id/seekBar3"
        android:layout_alignStart="@+id/seekBar3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Achiness"
        android:id="@+id/textView6"
        android:textColor="#ff000000"
        android:layout_below="@+id/seekBar3"
        android:layout_alignLeft="@+id/seekBar4"
        android:layout_alignStart="@+id/seekBar4" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:id="@+id/yourChoice1"
        android:layout_below="@+id/seekBar"
        android:layout_alignRight="@+id/seekBar"
        android:layout_alignEnd="@+id/seekBar"
        android:layout_marginRight="27dp"
        android:layout_marginEnd="27dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:id="@+id/yourChoice2"
        android:layout_alignTop="@+id/textView5"
        android:layout_alignLeft="@+id/yourChoice1"
        android:layout_alignStart="@+id/yourChoice1" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:id="@+id/yourChoice3"
        android:layout_below="@+id/seekBar3"
        android:layout_alignLeft="@+id/yourChoice2"
        android:layout_alignStart="@+id/yourChoice2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:id="@+id/yourChoice4"
        android:layout_below="@+id/seekBar4"
        android:layout_alignLeft="@+id/yourChoice3"
        android:layout_alignStart="@+id/yourChoice3" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Submit"
        android:onClick="activityFinish"
        android:id="@+id/button2"
        android:layout_marginTop="41dp"
        android:layout_below="@+id/yourChoice4"
        android:layout_alignRight="@+id/yourChoice4"
        android:layout_alignEnd="@+id/yourChoice4" />


</RelativeLayout>
